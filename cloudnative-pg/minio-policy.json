{
 "Version": "2012-10-17",
 "Statement": [
  {
   "Sid": "CloudNativePGStorage",
   "Effect": "Allow",
   "Action": [
    "s3:DeleteObject",
    "s3:GetObject",
    "s3:ListBucket",
    "s3:PutObject"
   ],
   "Resource": [
    "arn:aws:s3:::cloudnative-pg-backups/*",
    "arn:aws:s3:::cloudnative-pg-backups"
   ]
  }
 ]
}